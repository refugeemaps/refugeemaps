.print
  overflow-y: scroll
  .actions,
  .menu,
  .map,
  .menu__content,
  .sidebar__obfuscator,
  .infowindow,
  .filters
    display: none
  .infowindow
    display: none
  .filters
    display: none

.print-view
  position: absolute
  top: 0
  left: 0
  right: 0
  bottom: 0
  background: white
  display: none
  overflow-y: scroll
  z-index: 100
  .print &
    display: block
  &__image
    width: 100%
  &__list
    font-size: 17px
    padding: 10px
    &__item
      padding: 5px
      margin-top: 5px
      &__index
        margin-left: 10px
        font-weight: 700
      &__name
        margin-left: 10px
        font-weight: 700
      &__address
        font-size: 15px

